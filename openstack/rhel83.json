{
    "builders": [
        {
            "type": "openstack",
            "identity_endpoint": "https://rhos-d.infra.prod.upshift.rdu2.redhat.com:13000/v3",
            "cloud": "psi",
            "image_name": "osbuildci-rhel83-{{isotime \"20060102-150405\"}}",
            "source_image": "98f4299c-398a-4e7f-9372-f107340920b8",
            "flavor": "ci.m1.small",
            "image_visibility": "private",
            "networks": [
                "316eeb47-1498-46b4-b39e-00ddf73bd2a5"
            ],
            "ssh_username": "cloud-user"
        }
    ],
    "provisioners": [
      {
        "type": "file",
        "source": "prepare-openstack.sh",
        "destination": "/tmp/prepare.sh"
      },
      {
        "type": "file",
        "source": "files/rhel-8.tpl",
        "destination": "/tmp/rhel-8.tpl"
      },
      {
        "type": "shell",
        "inline": [
          "sudo /tmp/prepare.sh",
          "rm -f /tmp/prepare.sh"
        ]
      }
    ]
}
